      "type": "string",
      "length": 50
    },
    "lastName": {
      "type": "string",
      "length": 50
    },
    "middleName": {
      "type": "string",
      "length": 50
    },
    "trainingLevel": {
      "type": "string",
      "length": 50
    },
    "speciality": {
      "type": "string",
      "length": 50
