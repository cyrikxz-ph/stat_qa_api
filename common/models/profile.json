{
  "name": "profile",
  "base": "PersistedModel",
  "idInjection": true,
  "indexes": {
    "userId_index": {
      "keys": {
        "userId": 1
      }
    }
  },
  "options": {
    "validateUpsert": true
  },
  "hidden": [
    "userId",
    "specializationId",
    "trainingLvlId"
  ],
  "properties": {
    "firstName": {
      "type": "string",
      "length": 50
    },
    "lastName": {
      "type": "string",
      "length": 50
    },
    "middleName": {
      "type": "string",
      "length": 50
    },
    "verified": {
      "type": "boolean",
      "default": false
    }
  },
  "validations": [],
  "relations": {
    "specialization": {
      "type": "belongsTo",
      "model": "specialization",
      "foreignKey": ""
    },
    "trainingLvl": {
      "type": "belongsTo",
      "model": "trainingLevel",
      "foreignKey": ""
    }
  },
  "scope": {
    "include": [
      "specialization",
      "trainingLvl"
    ]
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "DENY"
    },
    {
      "accessType": "READ",
      "principalType": "ROLE",
      "principalId": "$owner",
      "permission": "ALLOW"
    }
  ],
  "methods": {}
}
